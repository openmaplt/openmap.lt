      "source-layer": "poi",
      "minzoom": 7,
      "maxzoom": 11,
      "layout": {
        "text-field": "{name}",
        "text-size": 12,
        "text-max-width": 9,
        "text-anchor": "top",
        "text-offset": [0, 0.6],
        "icon-image": "park",
        "text-font": ["Roboto Condensed Italic"],
        "text-padding": 1

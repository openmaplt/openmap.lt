      "maxzoom": 20,
      "filter": ["all", ["==", "kind", "country"]],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#eb9dcb",
        "line-width": { "base": 2, "stops": [[1, 1], [7, 3]] },
        "line-translate-anchor": "map"
      }
    },
    {

{
  "version": 8,
  "name": "openmap.lt",
  "metadata": { "maputnik:renderer": "mbgljs" },
  "sources": {
    "orto": {
      "type": "raster",
      "attribution": "© NŽT ORT10LT",
      "tiles": ["https://ort10lt.openmap.lt/g16/{z}/{x}/{y}.jpeg"],
      "tileSize": 256
    },
    "osm": {
      "type": "vector",
      "url": "https://openmap.lt/capabilities/all.json"
    },
    "topo": {
      "type": "vector",
      "url": "https://openmap.lt/capabilities/topo.json"
    },
    "speed": {
      "type": "vector",
      "url": "https://openmap.lt/capabilities/speed.json"
    }
  },
  "sprite": "https://openmap.lt/sprites/topo",
  "glyphs": "https://openmap.lt/glyphs/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "overlay",
      "type": "raster",
      "source": "orto",
      "minzoom": 0,
      "maxzoom": 20,
      "layout": {
        "visibility": "visible"
      }
    },
    {
      "id": "waterway-stream-direction",
      "type": "line",
      "source": "osm",
      "source-layer": "water",
      "minzoom": 16,
      "maxzoom": 24,
      "filter": ["all", ["in", "kind", "stream", "canal"]],
      "layout": { "line-join": "round", "line-cap": "round" },
      "paint": {
        "line-width": 16,
        "line-pattern": "stream",
        "line-opacity": 0.6,
        "line-translate": [0, 0]
      }
    },
    {
      "id": "waterway-river-direction",
      "type": "line",
      "source": "osm",
      "source-layer": "water",
      "minzoom": 15,
      "maxzoom": 24,
      "filter": ["all", ["==", "kind", "river"], ["==", "virtual", "N"]],
      "layout": { "line-join": "round", "line-cap": "round" },
      "paint": {
        "line-width": { "base": 1.55, "stops": [[4, 0.25], [20, 30]] },
        "line-pattern": "river",
        "line-opacity": 0.6
      }
    },
    {
      "id": "boundary-country",
